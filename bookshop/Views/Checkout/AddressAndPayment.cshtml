@model bookshop.order

@{
    ViewBag.Title = "填写您的收货信息";
}




@using (Html.BeginForm())
{
    <h2>收货信息填写</h2>
    <fieldset>
        <legend>收货信息</legend>
        @Html.EditorForModel()
    </fieldset>

    <div class="form-group">
    <input type="submit" value="提交" class="btn btn-default" />  
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
